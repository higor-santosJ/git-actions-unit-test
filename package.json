{
  "name": "git-actions-unit-test",
  "version": "1.0.0",
  "description": "A POC for validating unit tests with github actions",
  "main": "index.js",
  "scripts": {
    "test:unit": "npx jest --passWithNoTests --config=jest.unit.config.js",
    "test:int": "npx jest int --runInBand --passWithNoTests --watch --config=jest.int.config.js",
    "dev": "npx nodemon",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint --fix ."
  },
